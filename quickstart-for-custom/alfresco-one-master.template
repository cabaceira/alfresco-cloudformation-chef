                    "default": "Index Nodes Max Size"
                },
                "IndexNodesMinSize": {
                    "default": "Index Nodes Min Size"
                },
                "ElasticSearchMonInstanceType": {
                    "default": "ElasticSearch Monitoring Server Instance Type"
                },
                "ElasticSearchMigInstanceType": {
                    "default": "ElasticSearch Migration Server Instance Type"
                },
                "KeyPairName": {
